		<queryString>
			<![CDATA[select INTITULES as "INTITULES",CODES as "CODES",CC "CC",EF "EF",(TOTAL*20)/100 as "MOYENNESUR20",RATRAPPAGE from public.etudiant,
(select
 mat,cours.nom as INTITULES,cours.codesperso as CODES,cours.credits as CREDITS,cours.semestre as ns ,CC,EF,RATRAPPAGE,total,COTE,POINTS,MENTION,cours.semestre as Cs,cours.id_anee as Cann,cours.id_niveaux as cNiv from public.cours,
(select
 notes.codes as coden,notes.matricule as mat,notes.cct as CC,
 notes.total as TOTAL,notes.cote as COTE,notes.points as POINTS,notes.mention as MENTION,notes.examen as EF,notes.ratrappage as RATRAPPAGE
 from public.notes where notes.matricule = $P{matriculeTable}) as noteparEtud
where cours.codes=coden and cours.id_anee='2014/2015' AND cours.semestre='1') as noteParsCourDETUDetaEFesemestre
where etudiant.matricule=mat and etudiant.id_niveaux=cNiv;]]>
		</queryString>
		<field name="INTITULES" class="java.lang.String"/>
		<field name="CODES" class="java.lang.String"/>
		<field name="CC" class="java.lang.Double"/>
		<field name="EF" class="java.lang.Double"/>
		<field name="MOYENNESUR20" class="java.lang.Double"/>
		<field name="ratrappage" class="java.lang.Double"/>
		<variable name="moyennesurTotal" class="java.lang.Float" calculation="Average">
			<variableExpression><![CDATA[$F{MOYENNESUR20}]]></variableExpression>
		</variable>
		<variable name="detteSem1" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{MOYENNESUR20}<10?1:0]]></variableExpression>
		</variable>
		<variable name="putSem1" class="java.lang.String" incrementType="Report">
			<variableExpression><![CDATA[$P{MoyenneMAP1}.put("KEY",$V{moyennesurTotal})]]></variableExpression>
			<initialValueExpression><![CDATA[]]></initialValueExpression>
